{
    "name": "aztech/socket",
    "description": "Simple socket wrapper used by other components, not really a standalone component",
    "license": "MIT",
    "keywords": [
        "aztech",
        "aztech-digital"
    ],
    "type": "library",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Aztech\\Socket\\": "src/"
        }
    },
    "require": {
        "php": ">= 5.4",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2",
        "squizlabs/php_codesniffer": "~1.5",
        "vektah/bugfree-dangerzone": "~0.2",
        "phpmd/phpmd": "~2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
